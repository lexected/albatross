consume SPACE: "\\s+";
IDENTIFIER: "[a-zA-Z_][a-zA-Z_0-9]*";
begin consume tokenize STRING:
	required consume STRING_START: "\"";
	begin produce STRING_CONTENTS:
		produce STRING_CONTENTS_CHARACTERS: "[^\\\\\"]*";
		consume STRING_CONTENTS_ESCAPES: "\\\\\\\\|\\\\\\\"|\\\\\\n|\\\\\\r";
	end;
	required consume STRING_END: "\"";
end;

end;