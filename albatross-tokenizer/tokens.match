consume SPACE: "\\s+";
IDENTIFIER: "[a-zA-Z_][a-zA-Z0-9_]*";
SEMICOLON: ";";
LBRACKET: "\\(";
RBRACKET: "\\)";
COMMA: ",";
LBRACE: "\\{";
RBRACE: "\\}";

begin consume tokenize STRING:
	required consume STRING_START: "\"";
		consume produce STRING_CONTENTS_CHARACTERS: "[^\"]*";
	required consume STRING_END: "\"";
end;

end;